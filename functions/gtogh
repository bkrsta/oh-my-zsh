gtogh() {
  if [ -e "$1" ]; then
    login=`git config --get github.user`
    token=`git config --get github.token`
    R=`curl -F "login=$login" -F "token=$token" -F "name=$1"  http://github.com/api/v2/json/repos/create`
    git remote add origin git@github.com:`echo "$R" | sed 's/.*github.com\///g' | sed 's/\".*//g'`.git
    git push -u origin master
    echo
  fi
}
